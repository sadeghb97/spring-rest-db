	<script type="text/javascript">
		var GSNavCurrent;
		var GSNavCurrentNo = 1;
		var GSNav;
		var GSNavInterval;

		function rotateGSVav()
		{
			GSNavCurrent.removeClass('active');

			if (GSNavCurrentNo == 5)
			{
				GSNavCurrent = $('#gamingSessions > li:first-child');
				GSNavCurrentNo = 1;
				clearInterval(GSNavInterval);
			}
			else
			{
				GSNavCurrent = GSNavCurrent.next();
				GSNavCurrentNo++;
			}

			GSNavCurrent.addClass('active');
			GSNav.css('width', GSNavCurrent.width()+5);
		}

					$(function()
			{
				GSNav = $('#gamingSessions');

				GSNavCurrent = $('#gamingSessions > li:first-child');

				GSNav.css('width', GSNavCurrent.width()+5);

				GSNavInterval = setInterval(function()
				{
					rotateGSVav();
				}, 1500);

			});
			</script>

